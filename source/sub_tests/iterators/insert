#include "list.hpp"
TEST_CASE("insert Methode") {
  List<int> list1{};
  for(int x=0;x!=10;++x){
	list1.push_front(x);
  }
   auto a = list1.insert(ListIterator<int>{list1.begin()},-1);
	REQUIRE(list1.size() == 11);
	REQUIRE(list1.front() == -1);

	 list1.insert(a,0);
	 REQUIRE(list1.size() == 12);
	 REQUIRE(list1.front() != -2);

	}